{
  "manifest_version": 2,
  "name": "Tracking Query Remover",
  "description": "Removes useless tracking information from url",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
